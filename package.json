{
  "name": "funcadelic",
  "version": "0.4.0",
  "description": "Functional Fundamentals are Fun!",
  "main": "dist/funcadelic.cjs.js",
  "module": "dist/funcadelic.esm.js",
  "browser": "dist/funcadelic.umd.js",
  "repository": "https://github.com/cowboyd/funcadelic.js.git",
  "author": "Charles Lowell <cowboyd@frontside.io>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rollup --config",
    "pretest": "npm run build",
    "test": "jest",
    "prepare": "npm test",
    "start": "jest --watch"
  },
  "files": [
    "README.md",
    "src",
    "dist"
  ],
  "dependencies": {
    "lodash.curry": "4.1.1",
    "object.getownpropertydescriptors": "2.0.3"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-jest": "22.4.3",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-env": "1.6.1",
    "jest": "22.4.3",
    "regenerator-runtime": "0.11.1",
    "rollup": "0.55.1",
    "rollup-plugin-babel": "3.0.3",
    "rollup-plugin-commonjs": "8.3.0",
    "rollup-plugin-filesize": "1.5.0",
    "rollup-plugin-node-resolve": "3.0.2"
  },
  "jest": {
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(js)$",
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "globalSetup": "./build.js",
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "moduleNameMapper": {
      "funcadelic": "<rootDir>/dist/funcadelic.umd.js"
    }
  }
}
